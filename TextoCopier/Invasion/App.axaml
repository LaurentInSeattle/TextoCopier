<Application 
    x:Class="Lyt.Invasion.App"
	xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    RequestedThemeVariant="Dark"
	xmlns:semi="https://irihi.tech/semi"
	xmlns:convertLib="clr-namespace:Lyt.Avalonia.Mvvm.Converters;assembly=Lyt.Avalonia.Mvvm"
	xmlns:convertLocal="clr-namespace:Lyt.Invasion.Converters"
	>
	<Application.Resources>
		<convertLib:EnumBooleanConverter x:Key="enumBooleanConverter" />
		<convertLocal:EnumColorConverter x:Key="enumColorConverter" />
		<convertLocal:EnumBrushConverter x:Key="enumBrushConverter" />
	</Application.Resources>
	<Application.Styles>
		<semi:SemiTheme />
	</Application.Styles>
</Application>
