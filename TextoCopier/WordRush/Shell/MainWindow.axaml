<Window 
    x:Class="Lyt.WordRush.Shell.MainWindow"
	xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="600"
    Icon="/Assets/Images/parole.ico"
    Title="Parole in Fretta  ~  Word Rush"
	Width="840" Height="600"
	MinWidth="400" MinHeight="360"
	SizeToContent="Manual"
	>
	<Grid>
		<Image
            Stretch="UniformToFill"
            Source="\Assets\Images\dark.jpg"
            Grid.ColumnSpan="999" Grid.RowSpan="999"
            />
		<Rectangle
			Fill="#48000020"
            Grid.ColumnSpan="999" Grid.RowSpan="999"
			/>
		<Viewbox
			x:Name="viewbox"
			VerticalAlignment="Stretch" HorizontalAlignment="Stretch"
	        >
		</Viewbox>
	</Grid>
</Window>
