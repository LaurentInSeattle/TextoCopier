<Application
	x:Class="Lyt.TextoCopier.App"
	xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    RequestedThemeVariant="Default"
	>
	<!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

	<Application.Styles>
		<FluentTheme />
		<StyleInclude Source="avares://TextoCopier/Assets/Icons/Icons.axaml" />
		<StyleInclude Source="avares://TextoCopier/Assets/Palette/DeepBluePalette.axaml" />
		<StyleInclude Source="avares://TextoCopier/Assets/Theme/TextBoxStyles.axaml" />
	</Application.Styles>

	<Application.Resources>
		<ResourceDictionary>
			<!--
			<FontFamily x:Key="InterV">avares://TextoCopier/Assets/Inter-V.ttf</FontFamily>
			-->
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/Assets/Icons/FluentSVGResourceDictionary.axaml"/>
				<ResourceInclude Source="/Assets/Theme/Buttons.axaml"/>
				<!--
				<ResourceInclude Source="/Assets/Languages/en-US.axaml" />
				<ResourceInclude Source="/Assets/Languages/fr-FR.axaml" />
				<ResourceInclude Source="/Assets/Languages/it-IT.axaml" />
				-->
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
